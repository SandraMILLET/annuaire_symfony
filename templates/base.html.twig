	<script src="https://unpkg.com/leaflet@1.6.0/dist/leaflet.js"
		integrity="sha512-gZwIG9x3wUXg2hdXF6+rVkLF/0Vi9U8D2Ntg4Ga5I5BZpVkVxlJWbSQtXPSiUTtC0TjtGOmxa1AJPuV0CPthew=="
		crossorigin=""></script>
	<script src="https://unpkg.com/leaflet-routing-machine@latest/dist/leaflet-routing-machine.js"></script>
	<!--bootstrap-->
	<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css"
		integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
	<link href="https://cdn.jsdelivr.net/npm/summernote@0.8.16/dist/summernote.min.css" rel="stylesheet">
	<!--icone-->
	<link href="https://cdn.lineicons.com/2.0/LineIcons.css" rel="stylesheet">
	<link rel="stylesheet" href="{{ asset('css/style.css') }}">
	<!--captcha-->
	<script src="https://www.google.com/recaptcha/api.js?render=<6LenNu0UAAAAAE5rXV2DFhJv0VMS4Ofe9VwLA8jH>"></script>
	<!--jquery-->
	<script src="https://code.jquery.com/jquery-3.5.1.min.js"
		integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
	<meta name="viewport" content="width=device-width, initial-scale=1.0">



	{# read and display just one flash message type #}
	{% for message in app.flashes('notice') %}
	<div class="col align-self-center flash-notice">
		{{ message }}
	</div>
	{% endfor %}



	{% block stylesheets %}{% endblock %}
	{% block headPerso %}{% endblock %}
	{# Key: histoire qu'on puise chacun implémenter des trucs nécessaire à sa propre page #}
</head>

<body>
	{% include "INC/header.html.twig" %}
	{% block body %}{% endblock %}
	{% block javascripts %}
	<!--popper-->
	<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"
		integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo"
		crossorigin="anonymous"></script>
	<!--summernote-->
	<script src="https://cdn.jsdelivr.net/npm/summernote@0.8.16/dist/summernote.min.js"></script>

	{% endblock %}

	{% include "INC/footer.html.twig" %}
	{% block scriptPerso %}{% endblock %}


</body>

</html>